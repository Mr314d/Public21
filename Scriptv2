local Section = Tab:NewSection("Unlock all Motorcyle For Free")

local player = game:GetService("Players").LocalPlayer

Section:NewButton("Unlock all Motorcycles", "It doesn't save, but I'm Rich", function()
	local player = player
	local playerName = player.Name
	if player then
		local vehicleSpawner = player:FindFirstChild("Vehicle_Spawner")
		if vehicleSpawner then
			local replicatedStorage = game:GetService("ReplicatedStorage")
			local allVehicles = replicatedStorage:FindFirstChild("AVehicles")
			if allVehicles then
				for _, vehicle in ipairs(allVehicles:GetChildren()) do
					vehicle:Clone().Parent = vehicleSpawner
				end
			else
				warn("AVehicles not found in ReplicatedStorage")
			end
		else
			warn("Vehicle_Spawner not found for player " .. playerName)
		end
	else
		warn("Player " .. playerName .. " not found")
	end
end)

local Tab = Window:NewTab("LocalPlayer")
local Section = Tab:NewSection("LocalPlayer")

Section:NewSlider("Player Speed", "makes you walk Faster", 500, 0, function(s)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewSlider("Player Jump", "makes you Jump Height", 500, 0, function(s)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

Section:NewButton("Anti afk", "Nothing", function()
	local VirtualUser = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:Connect(function()
		VirtualUser:CaptureController()
		VirtualUser:ClickButton2(Vector2.new())
	end)
end)

local Tab = Window:NewTab("Visual")
local Section = Tab:NewSection("Visual Means Not Fe")

local player = game:GetService("Players").LocalPlayer
local leaderstats = player:FindFirstChild("leaderstats")

Section:NewButton("unlimited Gears", "Man just a Visual", function()
	if leaderstats then
		local gears = leaderstats:FindFirstChild("Gears")
		if gears then
			gears.Value = 99999999999999999
		end
	end
end)

Section:NewButton("unlimited Wheelie", "Man just a Visual", function()
	if leaderstats then
		local Wheelie = leaderstats:FindFirstChild("Wheelie")
		if Wheelie then
			Wheelie.Value = 99999999999999999
		end
	end
end)

local Tab = Window:NewTab("Server")
local Section = Tab:NewSection("Server")

local player = game.Players.LocalPlayer

local function Rejoin()
	local placeId = game.PlaceId
	local jobId = game.JobId

	game:GetService("TeleportService"):TeleportToPlaceInstance(placeId, jobId, player)
end

Section:NewButton("Rejoin", "Just a Rejoin", function()
	Rejoin()
end)

local Tab = Window:NewTab("Setting")
local Section = Tab:NewSection("Information")

local userInputService = game:GetService("UserInputService")

local isMobile = userInputService.TouchEnabled

if isMobile then
	Section:NewLabel("Device Type: Mobile/Tablet")
else
	Section:NewLabel("Device Type: Computer/Pc")
end

local player = game:GetService("Players").LocalPlayer

Section:NewLabel("Player Name: " .. player.Name)

Section:NewLabel("Display Name: " .. player.DisplayName)

local player = game:GetService("Players").LocalPlayer

Section:NewLabel("Player ID: " .. player.UserId)

Section:NewLabel("Made By R_tnt21")
