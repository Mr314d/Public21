local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("[ALPHA] üèçÔ∏è Motorcycle Mayhem", "Ocean")

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Spawn Any Motorcyle")

local function SpawnMVAgustus()
	local args = {
		[1] = "MV Agustus"
	}

	game:GetService("ReplicatedStorage"):WaitForChild("SpawnVeh"):FireServer(unpack(args))
end

